              <enumeratedValue>
                <name>div2</name>
                <description>Divide by 2.</description>
                <value>1</value>
              </enumeratedValue>
              <enumeratedValue>
                <name>div4</name>
                <description>Divide by 4.</description>
                <value>2</value>
