                <value>0</value>
              </enumeratedValue>
              <enumeratedValue>
                <name>sleep</name>
                <description>Sleep Mode.</description>
                <value>1</value>
              </enumeratedValue>
              <enumeratedValue>
                <name>deepsleep</name>
                <description>DeepSleep Mode.</description>
                <value>2</value>
              </enumeratedValue>
              <enumeratedValue>
                <name>shutdown</name>
                <description>ShutDown Mode.</description>
                <value>3</value>
              </enumeratedValue>
              <enumeratedValue>
                <name>backup</name>
                <description>Backup Mode.</description>
                <value>4</value>
              </enumeratedValue>
            </enumeratedValues>
          </field>
