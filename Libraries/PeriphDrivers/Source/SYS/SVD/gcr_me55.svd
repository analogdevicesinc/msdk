              </enumeratedValue>
              <enumeratedValue>
                <name>deepsleep</name>
                <description>Powered down in DEEPSLEEP.</description>
                <value>1</value>
              </enumeratedValue>
            </enumeratedValues>
          </field>
          <field derivedFrom="ERFO_PD">
